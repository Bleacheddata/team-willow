ield\n                id=\"standard-basic\"\n                type=\"password\"\n                autoComplete=\"off\"\n                name=\"confirm_password\"\n                value={this.state.confirm_password}\n                onChange={this.onChange}\n                placeholder=\"Confirm Password\"\n                required\n            />\n            <br /><br />\n            <Button\n                className=\"button_style\"\n                variant=\"contained\"\n                color=\"primary\"\n                size=\"small\"\n                disabled={this.state.username == '' && this.state.password == ''}\n                onClick={this.register}\n            >\n              Register\n            </Button> &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;\n            <Link href=\"/\">\n              Login\n            </Link>\n          </div>\n        </div>\n    );\n  }\n}\n","/Users/Masood/OneDrive/Maynooth_4thYear_Hdip/Software Project/team-willow/react-app/src/login/Login.js",["48","49","50","51"],[],{"ruleId":"52","replacedBy":"53"},{"ruleId":"54","replacedBy":"55"},{"ruleId":"56","severity":1,"message":"57","line":4,"column":18,"nodeType":"58","messageId":"59","endLine":4,"endColumn":26},{"ruleId":"56","severity":1,"message":"60","line":6,"column":10,"nodeType":"58","messageId":"59","endLine":6,"endColumn":16},{"ruleId":"56","severity":1,"message":"61","line":6,"column":18,"nodeType":"58","messageId":"59","endLine":6,"endColumn":27},{"ruleId":"62","severity":1,"message":"63","line":41,"column":14,"nodeType":"64","messageId":"65","endLine":41,"endColumn":16},{"ruleId":"66","severity":1,"message":"67","line":41,"column":26,"nodeType":"68","messageId":"69","endLine":41,"endColumn":28},{"ruleId":"66","severity":1,"message":"67","line":41,"column":41,"nodeType":"68","messageId":"69","endLine":41,"endColumn":43},{"ruleId":"62","severity":1,"message":"70","line":41,"column":63,"nodeType":"64","messageId":"65","endLine":41,"endColumn":65},{"ruleId":"71","severity":1,"mes