age.getItem(\"token\")\n      }\n    })\n    .then((res) => {\n      this.setState({ cardPack: []});\n      this.setState({\n                cardPack: res.data.concat(),\n                packCount : this.state.packCount-1\n           });\n      \n    })\n    .catch((err) => {\n      console.error(err)\n    })\n  }\n  else if (this.state.packCount<=0) {\n    event.target.setAttribute(\"disabled\", \"disabled\");\n  }\n  \n\n  \n  \n  }\n  \n\n  }\n  ","/Users/Masood/OneDrive/Maynooth_4thYear_Hdip/Software Project/team-willow/react-app/src/login/Register.js",["46","47","48"],[],"/Users/Masood/OneDrive/Maynooth_4thYear_Hdip/Software Project/team-willow/react-app/src/login/Login.js",["49","50","51"],[],"/Users/Masood/OneDrive/Maynooth_4thYear_Hdip/Software Project/team-willow/react-app/src/NaviBar.js",[],[],["52","53"],{"ruleId":"54","severity":1,"message":"55","line":4,"column":18,"nodeType":"56","messageId":"57","endLine":4,"endColumn":26},{"ruleId":"54","severity":1,"message":"58","line":6,"column":10,"nodeType":"56","messageId":"57","endLine":6,"endColumn":16},{"ruleId":"54","severity":1,"message":"59","line":6,"column":18,"nodeType":"56","messageId":"57","endLine":6,"endColumn":27},{"ruleId":"60","severity":1,"message":"61","line":42,"column":14,"nodeType":"62","messageId":"63","endLine":42,"endColumn":16},{"ruleId":"64","severity":1,"message":"65","line":42,"column":26,"nodeType":"66","messageId":"67","endLine":42,"endColumn":28},{"ruleId":"64","severity":1,"message":"65","line":42,"column":41,"nodeType":"66","messageId":"67","endLine":42,"endColumn":43},{"ruleId":"60","severity":1,"message":"68","line":42,"column":63,"nodeType":"62","messageId":"63","endLine":42,"endColumn":65},{"ruleId":"69","severity":1,"message":"70","line":95,"column":3,"nodeType":"71","messageId":"72","endLine":97,"endColumn":4},{"ruleId":"69","severity":1,"message":"70","line":110,"column":3,"nodeType":"71","messageId":"72","endLine":112,"endColumn":4},{"ruleId":"54","severity":1,"message":